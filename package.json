  "license": "MIT",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "serve:url-storefront": "ng serve url-storefront",
    "serve:url-storefront-api": "ng serve api",
    "serve:url-storefront:all": "ng serve-with-api url-storefront",
    "serve:docker-compose:all": "docker-compose up --build  headless-store",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build:url-storefront:prod": "nx build url-storefront --prod",
    "build": "nx build",
    "test": "nx test",
    "dep-graph": "nx dep-graph",
    "docker:rm-web": "docker stop web && docker rm web",
    "docker:rm-api": "docker stop api && docker rm api",
    "docker:rmi-web": "docker rmi nxproduction_web",
    "docker:rmi-api": "docker rmi nxproduction_api0 && docker rmi nxproduction_api1",
    "docker:clear": "npm run docker:rm-web && npm run docker:rmi-web && docker:rm-api && docker:rmi-api",
    "docker:serve-prod": "npm run build -- --prod && ./docker/docker-compose up"
  },
  "private": true,
